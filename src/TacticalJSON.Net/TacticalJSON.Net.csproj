<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.1;net4.6.2;net4.7.2;net6.0</TargetFrameworks>
		<Description>.NET implementation of the TacticalJSON format, based on GeoJSON.Net.</Description>
		<Authors>maelscuttle</Authors>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>1.0.0</Version>
		<LangVersion>9.0</LangVersion>
		<PackageProjectUrl>https://github.com/maelscuttle/TacticalJSON.Net</PackageProjectUrl>
		<RepositoryUrl>https://github.com/maelscuttle/TacticalJSON.Net.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>geojson;geo;json;geolocation;tacticaljson;milsymbol;milgeo</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageReleaseNotes>Initial release</PackageReleaseNotes>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="GeoJSON.Net" Version="1.2.19" />
	</ItemGroup>

</Project>